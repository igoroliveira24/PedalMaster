CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_Insert_ItensPedidos`(
spquantidade int ,
spdesconto decimal(7,2),
spfkPedidos int ,
spfkProdutos int 
)
begin 
insert into ItensPedidos
values(
0,
spquantidade,
spdesconto,
(select Preco from Produto where pk_idProdutos = spfkProdutos),
spfkPedidos,
spfkProdutos
);
end ;;
