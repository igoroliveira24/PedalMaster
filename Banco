spnumero int,
spvalidade date,
sptipo varchar(10),
spcvv varchar(3)
)
begin
insert into Cartoes
values(
0,
spnome,
spbandeira,
spnumero,
spvalidade,
sptipo,
spcvv,
default
);
insert into CartoesClientes
values(
0,
sp_idClientes,
last_insert_id()
);
end ;;
